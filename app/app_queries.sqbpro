view_pk="_rowid_"><sort/><column_widths><column index="1" value="101"/><column index="2" value="101"/><column index="3" value="300"/><column index="4" value="101"/><column index="5" value="101"/><column index="6" value="101"/><column index="7" value="122"/><column index="8" value="106"/><column index="9" value="101"/><column index="10" value="101"/><column index="11" value="101"/><column index="12" value="101"/><column index="13" value="300"/></column_widths><filter_values/><conditional_formats/><row_id_formats/><display_formats/><hidden_columns/><plot_y_axes/><global_filter/></table></browse_table_settings></tab_browse><tab_sql><sql name="Fights per year">select year, count(DISTINCT Event) as &quot;#Events&quot;, count(id) as &quot;Total Fights&quot;, count(distinct myRobot) as &quot;#Robots&quot;, count(case when myWin=&quot;true&quot; then 1 end) as &quot;Wins&quot;, count(case when myWin=&quot;false&quot; then 1 end) as &quot;Losses&quot; from Fights group by year</sql><sql name="Unique Opponents">select year, count(id) as &quot;NumFights&quot;, count(DISTINCT opponentRobot) as &quot;# Opponents&quot; from Fights group by year</sql><sql name="Current Year Stats">select myRobot, count(distinct event) as &quot;Events&quot;, count(id) as &quot;NumFights&quot;, count(case when myWin=&quot;true&quot; then 1.0 end) as &quot;Wins&quot;, count(case when myWin=&quot;false&quot; then 1.0 end) as &quot;Losses&quot;, CAST(count(case when myWin=&quot;true&quot; then 1.0 end) AS DECIMAL(5,2))/(CAST(count(id) AS DECIMAL(5,2))) as &quot;Winrate&quot; from Fights where year=&quot;2024&quot; group by myRobot order by NumFights desc</sql><sql name="Unique Opponents (1)">select count(id) as &quot;NumFights&quot;, count(DISTINCT opponentRobot) as &quot;# Opponents&quot; from Fights where year = &quot;2024&quot;</sql><sql name="Career WL">select count(id) as &quot;NumFights&quot;, count(case when myWin=&quot;true&quot; then 1 end) as &quot;Wins&quot;, count(case when myWin=&quot;false&quot; then 1 end) as &quot;Losses&quot;, CAST(count(case when myWin=&quot;true&quot; then 1.0 end) AS DECIMAL(5,2))/(CAST(count(id) AS DECIMAL(5,2))) as &quot;Winrate&quot; from Fights</sql><sql name="Current Year Stats Aggregate">select count(id) as &quot;Total Fights this year&quot;, count(case when myWin=&quot;true&quot; then 1 end) as &quot;Wins&quot;, count(case when myWin=&quot;false&quot; then 1 end) as &quot;Losses&quot;, CAST(count(case when myWin=&quot;true&quot; then 1.0 end) AS DECIMAL(5,2))/(CAST(count(id) AS DECIMAL(5,2))) as &quot;Winrate&quot; from Fights where year=&quot;2024&quot;</sql><sql name="Finishes">select finish as &quot;place&quot;, count(finish) as &quot;count&quot; from Events where Finish not like &quot;N%&quot; group by place order by count desc </sql><sql name="Finishes This Year">select finish as &quot;place&quot;, count(finish) as &quot;count&quot; from Events where Finish not like &quot;N%&quot; and date like &quot;%2024%&quot; group by place order by count desc </sql><sql name="top opponents">select opponentRobot, count(opponentRobot) as &quot;Times Fought&quot;, count(case when myWin=&quot;true&quot; then 1 end) as &quot;Wins&quot; from Fights where opponentRobot not like &quot;%Rumble&quot; group by opponentRobot  order by Wins desc </sql><sql name="worst matchups">select opponentRobot, count(opponentRobot) as &quot;Times Fought&quot; from Fights where opponentRobot not like &quot;%Rumble&quot; AND myWin=&quot;false&quot; group by opponentRobot order by &quot;Times Fought&quot; desc</sql><sql name="Number of Events">select robot, count(DISTINCT name) as &quot;count&quot; from Events group by robot order by &quot;count&quot; desc</sql><sql name="record vs">select * from Fights where opponentRobot REGEXP 'Cruncher|Meteor|Titan' order by year</sql><sql name="Fights per bot">select myRobot, count(DISTINCT Event) as &quot;#Events&quot;, count(id) as &quot;Total Fights&quot;, count(distinct myRobot) as &quot;#Robots&quot;, count(case when myWin=&quot;true&quot; then 1 end) as &quot;Wins&quot;, count(case when myWin=&quot;false&quot; then 1 end) as &quot;Losses&quot; from Fights group by myrobot order by &quot;Total Fights&quot; desc</sql><current_tab id="11"/></tab_sql></sqlb_project>
